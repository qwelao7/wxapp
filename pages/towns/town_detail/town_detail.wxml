<!--logs.wxml-->
<scroll-view scroll-y class="container">
  <swiper indicator-dots="{{indicatorDots}}"
          autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" style="width: 100%">
    <block wx:for="{{town.imgUrls}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image"/>
      </swiper-item>
    </block>
  </swiper>
  <view class="page__bd black">
    <view class="weui-article">
      <view class="weui-article__h1">
        {{town.name}}
      </view>
      <view class="weui-article__section">
        <view class="weui-article__title" style="margin-bottom: .9em">
          <i class="iconfont icon-ADDRESS"></i>
          {{town.location}}
        </view>
        <view class="weui-article__section">
          <view class="weui-article__h4 gray">楼盘简介</view>
          <view class="weui-article__p town_detail">
            {{town.detail}}
          </view>
        </view>

      </view>
    </view>
  </view>
</scroll-view>
